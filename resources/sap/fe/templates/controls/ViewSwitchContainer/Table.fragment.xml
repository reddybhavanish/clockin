<core:FragmentDefinition
	xmlns:template="http://schemas.sap.com/sapui5/extension/sap.ui.core.template/1"
	xmlns="sap.m"
	xmlns:macro="sap.fe.macros"
	xmlns:core="sap.ui.core"
	xmlns:unittest="http://schemas.sap.com/sapui5/preprocessorextension/sap.fe.unittesting/1"
	template:require="{
		ID: 'sap/fe/core/helpers/StableIdHelper'
	}"
>


	<macro:Table
		unittest:id="TablePropertyExpressionTest"
		id="{presentationContext>tableConfiguration/annotation/id}"
		collection="{presentationContext>tableConfiguration/annotation/collection}"
		presentation="{annotationPath>}"
		quickFilter="{presentationContext>tableConfiguration/control/quickFilter}"
		parentEntitySet="{entitySet>}"
		columnSettings="{viewData>/controlConfiguration}"
		navigationPath="{presentationContext>tableConfiguration/annotation/navigationPath}"
		editMode="{presentationContext>tableConfiguration/annotation/editMode}"
		createMode="{presentationContext>tableConfiguration/annotation/createMode}"
		busy="{presentationContext>tableConfiguration/annotation/busy}"
		headerVisible="{presentationContext>tableConfiguration/control/headerVisible}"
		enableExport="{presentationContext>tableConfiguration/control/enableExport}"
		tableType="{presentationContext>tableConfiguration/control/type}"
		rowPress="{presentationContext>tableConfiguration/annotation/row/press}"
		rowAction="{presentationContext>tableConfiguration/annotation/row/action}"
		selectionMode="{presentationContext>tableConfiguration/annotation/selectionMode}"
		onCallAction=".editFlow.onCallAction"
		onDataFieldForIBN=".handlers.onDataFieldForIntentBasedNavigation"
		onCreate=".editFlow.createDocument"
		onPatchSent=".editFlow.handlePatchSent"
		onPatchCompleted=".editFlow.handlePatchCompleted"
		showCreate="{presentationContext>tableConfiguration/annotation/show/create}"
		showDelete="{presentationContext>tableConfiguration/annotation/show/delete}"
		onDelete=".editFlow.deleteMultipleDocuments"
		selectedContextsModel="localUI"
		groupId="$auto.Workers"
		threshold="{presentationContext>threshold}"
		onDataReceived=".editFlow.handleErrorOfTable"
		noDataText="{= ${presentationContext>tableConfiguration/annotation/isEntitySet} ? ${sap.fe.i18n>TABLE_NO_DATA_TEXT} : undefined }"
		creationMode="{presentationContext>tableConfiguration/control/creationMode}"
		disableAddRowButtonForEmptyData="{presentationContext>tableConfiguration/control/disableAddRowButtonForEmptyData}"
		createAtEnd="{presentationContext>tableConfiguration/control/createAtEnd}"
		autoBindOnInit="{presentationContext>tableConfiguration/annotation/autoBindOnInit}"
		onChange=".handlers.onFieldValueChange"
		editModePropertyBinding="ui>/editMode"
		enableControlVM="{presentationContext>tableConfiguration/annotation/enableControlVM}"
		p13nMode="{presentationContext>tableConfiguration/annotation/p13nMode}"
		filter="{presentationContext>tableConfiguration/annotation/filterId}"
		actions="{presentationContext>tableConfiguration/actions}"
		variantSelected=".handlers.onVariantSelected"
		variantSaved=".handlers.onVariantSaved"
	>
		</macro:Table>
</core:FragmentDefinition>
