<?xml version="1.0" encoding="UTF-8" ?>
<View
  controllerName="sap.ushell.components.tiles.action.ActionTile"
  height="100%"
  width="100%"
  xmlns="sap.ui.core"
  xmlns:ushell="sap.ushell.ui.tile"
>

<ushell:TileBase
    id="actionTile"
    title="{i18n>tile.display.action_chip}"
    subtitle="{/displayText}"
    icon="sap-icon://action"
    press="onPress"
  >
</ushell:TileBase>

</View>
