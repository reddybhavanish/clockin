<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	class="sapUiSizeCompact">
	<SelectDialog
	    id="approverDialog"
		title="{approver>/DialogTitle}"
		noDataText="{i18n>nodata}"
		search="handleSearch"
		confirm="handleConfirm"
		cancel="handleCancel"
		items="{approver>/}">
		<StandardListItem
			title="{approver>Name}"
			description="{approver>EmployeeID}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>