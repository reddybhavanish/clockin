<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" class="sapUiSizeCompact">
	<SelectDialog id="messagesDialog" title="{i18n>messagesText}" noDataText="{i18n>nodata}" cancel="handleCancel" confirm="onConfirm" items="{Messages>/}">
		<StandardListItem title="{Messages>MessageText}" description="{Messages>DispDate}" type="Navigation" press="onDetailPress">
			<customData>
				<core:CustomData key="MessageDate" value="{Messages>MessageDate}"/>
			</customData>
		</StandardListItem>
	</SelectDialog>
</core:FragmentDefinition>