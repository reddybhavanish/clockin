<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	class="sapUiSizeCompact">
	<SelectDialog
	    id="valueHelpDialog"
		title="{valueHelpSet>/DialogTitle}"
		noDataText="{i18n>nodata}"
		search="handleSearch"
		confirm="handleConfirm"
		cancel="handleCancel"
		items="{valueHelpSet>/}">
		<StandardListItem
			title="{valueHelpSet>title}"
			description="{valueHelpSet>description}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>